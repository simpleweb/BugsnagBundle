{
    "name": "simpleweb/bugsnag-bundle",
    "type": "symfony-bundle",
    "description": "Bugsnag error reporting integration for Symfony2",
    "keywords": ["symfony2", "bugsnag", "exceptions", "errors", "logging", "tracking"],
    "license": "MIT",
    "authors": [
        {
            "name": "Steve Lacey",
            "email": "steve@simpleweb.co.uk"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "bugsnag/bugsnag": "2.*",
        "symfony/symfony": ">=2.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7"
    },
    "autoload": {
        "psr-4": { "Simpleweb\\BugsnagBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
